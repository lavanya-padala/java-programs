package redBlackTree;
import java.util.*;

enum Color
{
	Red,
	Black
}
class Node
{
	public Node left;
	public Node right;
	public Node parent;
	public Color color;
	public int data;
}
class RedBlackTree
{
	private Node root;
	private Node TNULL;
	RedBlackTree(){
		TNULL.left=null;
		TNULL.right=null;
		root=null;
		TNULL.color=Color.Black;
	}
}